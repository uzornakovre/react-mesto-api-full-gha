             formData,\n                            onUpdateUser,\n                            isLoading,\n                            onOverlayClick }) {\n\n  const currentUser = useContext(CurrentUserContext);\n\n  function handleSubmit(evt) {\n    evt.preventDefault();\n    onUpdateUser({\n      name:  formData.values.name,\n      about: formData.values.description\n    });\n  }\n \n  useEffect(() => {\n    formData.setValues({\n      name:        currentUser.name,\n      description: currentUser.about\n    })\n  }, [isOpen]);\n\n  return (\n    <PopupWithForm name={'edit-profile'}\n                   title={'Редактировать профиль'}\n                   isOpen={isOpen}\n                   onClose={onClose}\n                   onSubmit={handleSubmit}\n                   isValid={formData.isValid}\n                   onOverlayClick={onOverlayClick}\n                   buttonText={isLoading ? 'Сохранение...' : 'Сохранить'}\n    >\n      <input type=\"text\"\n             className={`popup__form-input popup__form-input_content_name ${\n               formData.errors.name && 'popup__form-input_error'\n             }`}\n             id=\"name\"\n             name=\"name\"\n             placeholder=\"Имя\"\n             minLength=\"2\"\n             maxLength=\"40\"\n             value={formData.values.name || ''}\n             onChange={formData.handleChange}\n             required\n      />\n      <span className=\"popup__form-input-error popup__form-input-error_content_name\">\n        {formData.errors.name}\n      </span>\n      <input type=\"text\"\n             className={`popup__form-input popup__form-input_content_job ${\n               formData.errors.description && 'popup__form-input_error'\n             }`}\n             id=\"job\"\n             name=\"description\"\n             placeholder=\"Профессия\"\n             minLength=\"2\"\n             maxLength=\"200\"\n             value={formData.values.description || ''}\n             onChange={formData.handleChange}\n             required\n      />\n      <span className=\"popup__form-input-error popup__form-input-error_content_job\">\n        {formData.errors.description}\n      </span>\n    </PopupWithForm>\n  )\n}\n\nexport default EditProfilePopup;","/Users/uzornakovre/Desktop/WEBDEV/Обучение/Яндекс Практикум/Practice/dev/react-mesto-auth/src/components/Footer.jsx",[],[],"/Users/uzornakovre/Desktop/WEBDEV/Обучение/Яндекс Практикум/Practice/dev/react-mesto-auth/src/components/Header.jsx",[],[],"/Users/uzornakovre/Desktop/WEBDEV/Обучение/Яндекс Практикум/Practice/dev/react-mesto-auth/src/components/ImagePopup.jsx",[],[],"/Users/uzornakovre/Desktop/WEBDEV/Обучение/Яндекс Практикум/Practice/dev/react-mesto-auth/src/components/InfoToolTip.jsx",[],[],"/Users/uzornakovre/Desktop/WEBDEV/Обучение/Яндекс Практикум/Practice/dev/react-mesto-auth/src/components/Login.jsx",[],[],"/Users/uzornakovre/Desktop/WEBDEV/Обучение/Яндекс Практикум/Practice/dev/react-mesto-auth/src/components/Main.jsx",[],[],"/Users/uzornakovre/Desktop/WEBDEV/Обучение/Яндекс Практикум/Practice/dev/react-mesto-auth/src/components/PopupWithForm.jsx",[],[],"/Users/uzornakovre/Desktop/WEBDEV/Обучение/Яндекс Практикум/Practice/dev/react-mesto-auth/src/components/ProtectedRoute.jsx",[],[],"/Users/uzornakovre/Desktop/WEBDEV/Обучение/Яндекс Практикум/Practice/dev/react-mesto-auth/src/components/Register.jsx",[],[],"/Users/uzornakovre/Desktop/WEBDEV/Обучение/Яндекс Практикум/Practice/dev/react-mesto-auth/src/contexts/CurrentUserContext.jsx",[],[],"/Users/uzornakovre/Desktop/WEBDEV/Обучение/Яндекс Практикум/Practice/dev/react-mesto-auth/src/components/Card.jsx",[],[],{"ruleId":"117","severity":1,"message":"118","line":88,"column":6,"nodeType":"119","endLine":88,"endColumn":16,"suggestions":"120"},{"ruleId":"117","severity":1,"message":"121","line":269,"column":6,"nodeType":"119","endLine":274,"endColumn":24,"suggestions":"122"},{"ruleId":"117","severity":1,"message":"123","line":28,"column":6,"nodeType":"119","endLi